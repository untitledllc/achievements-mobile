<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:p1="http://schemas.android.com/apk/res/android"
    p1:minWidth="25px"
    p1:minHeight="150dp"
    p1:layout_width="fill_parent"
    p1:layout_height="150dp"
    p1:id="@+id/relativeLayout1"
    p1:background="#ffffffff">
    <ImageView
        p1:layout_width="fill_parent"
        p1:layout_height="2dp"
        p1:id="@+id/imageView1"
        p1:background="#ff4a4a4a" />
    <ImageView
        p1:layout_width="70dp"
        p1:layout_height="70dp"
        p1:id="@+id/AchiveImageView"
        p1:layout_marginLeft="20dp"
        p1:layout_marginTop="20dp"
        p1:drawingCacheQuality="low" />
    <Button
        p1:layout_width="25dp"
        p1:layout_height="25dp"
        p1:layout_below="@id/AchiveImageView"
        p1:id="@+id/facebook_button"
        p1:layout_marginLeft="10dp"
        p1:layout_marginTop="20dp"
        p1:textSize="8dp"
        p1:background="@drawable/Table_socialNetwork_FB_btn_norm" />
    <Button
        p1:layout_width="25dp"
        p1:layout_height="25dp"
        p1:layout_toRightOf="@id/facebook_button"
        p1:id="@+id/twitter_button"
        p1:layout_below="@+id/AchiveImageView"
        p1:layout_marginLeft="10dp"
        p1:layout_marginTop="20dp"
        p1:textSize="8dp"
        p1:background="@drawable/Table_socialNetwork_TW_btn_norm" />
    <Button
        p1:layout_width="25dp"
        p1:layout_height="25dp"
        p1:layout_toRightOf="@id/twitter_button"
        p1:id="@+id/vk_button"
        p1:layout_below="@+id/AchiveImageView"
        p1:layout_marginLeft="10dp"
        p1:layout_marginTop="20dp"
        p1:textSize="8dp"
        p1:background="@drawable/Table_socialNetwork_BK_btn_norm" />
    <RelativeLayout
        p1:minWidth="25px"
        p1:minHeight="25px"
        p1:layout_width="fill_parent"
        p1:layout_height="fill_parent"
        p1:layout_toRightOf="@id/vk_button"
        p1:id="@+id/relativeLayout2">
        <TextView
            p1:text="Large Text"
            p1:textAppearance="?android:attr/textAppearanceLarge"
            p1:layout_width="wrap_content"
            p1:layout_height="wrap_content"
            p1:id="@+id/AchiveNameTextView"
            p1:textColor="#ffff0000"
            p1:layout_marginLeft="10dp"
            p1:layout_marginTop="20dp" />
        <TextView
            p1:text="To get this achievement you should make first blood donation on №153 and №2 clinic stations. Scan QR-code from individual clinic sertificate."
            p1:textAppearance="?android:attr/textAppearanceSmall"
            p1:layout_width="fill_parent"
            p1:layout_height="70dp"
            p1:layout_below="@id/AchiveNameTextView"
            p1:id="@+id/AchiveDescriptionTextView"
            p1:layout_marginLeft="10dp"
            p1:layout_marginRight="10dp"
            p1:textColor="#ff474747"
            p1:textSize="11dp" />
        <TextView
            p1:text="Got at 16.11.2012"
            p1:textAppearance="?android:attr/textAppearanceSmall"
            p1:layout_width="wrap_content"
            p1:layout_height="wrap_content"
            p1:layout_below="@id/AchiveDescriptionTextView"
            p1:id="@+id/AchiveReceiveDateTextView"
            p1:layout_marginLeft="10dp"
            p1:textColor="#ff00bcd9" />
    </RelativeLayout>
</RelativeLayout>