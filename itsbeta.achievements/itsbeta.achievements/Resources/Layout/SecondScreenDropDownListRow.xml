<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:p1="http://schemas.android.com/apk/res/android"
    p1:minWidth="25px"
    p1:minHeight="40dp"
    p1:layout_width="fill_parent"
    p1:layout_height="40dp"
    p1:id="@+id/relativeLayout1"
    p1:background="@drawable/Categories_popup_background">
    <TextView
        p1:text="CategotyName"
        p1:textAppearance="?android:attr/textAppearanceLarge"
        p1:layout_width="wrap_content"
        p1:layout_height="wrap_content"
        p1:id="@+id/CategNameTextView"
        p1:gravity="center_horizontal"
        p1:layout_marginLeft="45dp"
        p1:layout_centerVertical="true"
        p1:textColor="#ff474747"
        p1:textSize="12dp" />
    <ImageView
        p1:src="@drawable/Categories_popup_check"
        p1:layout_width="30dp"
        p1:layout_height="25dp"
        p1:id="@+id/CheckImageView"
        p1:layout_marginLeft="7dp"
        p1:layout_marginTop="7.5dp" />
    <ImageView
        p1:layout_width="fill_parent"
        p1:layout_height="2dp"
        p1:id="@+id/imageView1"
        p1:layout_alignParentTop="true"
        p1:background="@drawable/TableAndButtons_divider_h" />
    <Button
        p1:layout_width="fill_parent"
        p1:id="@+id/check_button"
        p1:visibility="visible"
        p1:background="@android:color/transparent"
        p1:layout_alignParentLeft="true"
        p1:layout_alignParentTop="true"
        p1:layout_alignParentRight="true"
        p1:layout_alignParentBottom="true"
        p1:layout_height="38dp" />
</RelativeLayout>