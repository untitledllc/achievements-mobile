<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:p1="http://schemas.android.com/apk/res/android"
    p1:minWidth="25px"
    p1:minHeight="25px"
    p1:layout_width="wrap_content"
    p1:layout_height="170dp"
    p1:id="@+id/FristBadgeMainRelativeLayout">
    <ImageView
        p1:layout_width="wrap_content"
        p1:layout_height="170dp"
        p1:id="@+id/BadgeSheetImageView"
        p1:background="@drawable/Paper_background_middleMiddle"
        p1:minWidth="200dp"
        p1:layout_alignRight="@+id/textView2"
        p1:layout_alignLeft="@+id/textView2"
        p1:layout_marginLeft="-15dp"
        p1:layout_marginRight="-15dp" />
    <TextView
        p1:text="Информация"
        p1:textAppearance="?android:attr/textAppearanceLarge"
        p1:layout_width="wrap_content"
        p1:layout_height="wrap_content"
        p1:id="@+id/textView1"
        p1:layout_alignTop="@id/BadgeSheetImageView"
        p1:layout_alignLeft="@id/BadgeSheetImageView"
        p1:layout_marginLeft="15dp"
        p1:layout_marginTop="20dp"
        p1:textColor="#ff4a4a4a"
        p1:textSize="20dp"
        p1:typeface="normal" />
    <TextView
        p1:textAppearance="?android:attr/textAppearanceMedium"
        p1:layout_width="wrap_content"
        p1:layout_height="wrap_content"
        p1:id="@+id/textView2"
        p1:textColor="#ff4a4a4a"
        p1:textSize="16dp"
        p1:gravity="left"
        p1:minWidth="250dp"
        p1:layout_centerHorizontal="false"
        p1:text="Неверный код активации"
        p1:layout_marginLeft="15dp"
        p1:layout_marginRight="15dp"
        p1:typeface="normal"
        p1:layout_centerVertical="true" />
    <ImageView
        p1:id="@+id/imageView3"
        p1:background="@drawable/TableAndButtons_divider_h"
        p1:layout_width="match_parent"
        p1:layout_height="3dp"
        p1:layout_above="@+id/linearLayout1"
        p1:layout_alignRight="@+id/BadgeSheetImageView"
        p1:layout_alignLeft="@+id/BadgeSheetImageView" />
    <LinearLayout
        p1:orientation="horizontal"
        p1:minWidth="25px"
        p1:minHeight="25px"
        p1:layout_width="fill_parent"
        p1:layout_height="40dp"
        p1:id="@+id/linearLayout1"
        p1:layout_centerHorizontal="false"
        p1:layout_alignRight="@+id/BadgeSheetImageView"
        p1:layout_alignLeft="@+id/BadgeSheetImageView"
        p1:layout_alignBottom="@+id/BadgeSheetImageView">
        <Button
            p1:text="Ок"
            p1:id="@+id/readyButton"
            p1:background="@drawable/Paper_background_middleMiddle"
            p1:textSize="12dp"
            p1:textColor="#ff4e4e4e"
            p1:layout_width="fill_parent"
            p1:layout_height="40dp"
            p1:layout_weight="1" />
    </LinearLayout>
</RelativeLayout>